{
    "type": "postgres",
    "host": "localhost",
    "port": 5430,
    "username": "app",
    "password": "secret",
    "database": "app",
    "entities": ["dist/entities/*.entity.js"],
    "migrationsTableName": "custom_migration_table",
    "migrations": ["dist/database/migrations/*.js"],
